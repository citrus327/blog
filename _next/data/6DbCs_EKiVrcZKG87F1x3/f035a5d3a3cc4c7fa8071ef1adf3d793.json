\n\n  return result;\n}\n\nlet arr = [{ name: \"hao\" }, { name: \"peng\" }, { name: \"derek\" }];\n\nlet result = filter(arr, (item) => item.name === \"derek\");\n\nconsole.log(result);"]],"language":[["JavaScript"]]},"format":{"code_wrap":true},"created_time":1673275165531,"last_edited_time":1673276532937,"parent_id":"f035a5d3-a3cc-4c7f-a807-1ef1adf3d793","parent_table":"block","alive":true,"ignore_block_count":true,"created_by_table":"notion_user","created_by_id":"655f56a9-2482-4fed-ae6d-9f23ccbb923a","last_edited_by_table":"notion_user","last_edited_by_id":"655f56a9-2482-4fed-ae6d-9f23ccbb923a","space_id":"11f66c5f-6043-406b-b492-4dc267645668"}},"6fb55e71-bc40-454e-b229-77cd07b8a2cb":{"role":"reader","value":{"id":"6fb55e71-bc40-454e-b229-77cd07b8a2cb","version":1,"type":"sub_header","properties":{"title":[["Array.prototype.reduce"]]},"created_time":1673275165531,"last_edited_time":1673275165531,"parent_id":"f035a5d3-a3cc-4c7f-a807-1ef1adf3d793","parent_table":"block","alive":true,"ignore_block_count":true,"created_by_table":"notion_user","created_by_id":"655f56a9-2482-4fed-ae6d-9f23ccbb923a","last_edited_by_table":"notion_user","last_edited_by_id":"655f56a9-2482-4fed-ae6d-9f23ccbb923a","space_id":"11f66c5f-6043-406b-b492-4dc267645668"}},"021ecbcf-76b3-4074-8d13-2feeac4bb42c":{"role":"reader","value":{"id":"021ecbcf-76b3-4074-8d13-2feeac4bb42c","version":6,"type":"code","properties":{"title":[["function reduce(source, method, acc) {\n  if (acc === undefined) {\n    throw new Error(\"accumlator is undefined\");\n  }\n\n  let copy = acc;\n\n  source.forEach((curr, index, src) => {\n    copy = method(copy, curr, index, src);\n  });\n\n  return copy;\n}\n\nlet arr = [1, 2, 3];\n\nlet result = reduce(\n  arr,\n  (acc, curr, index, src) => {\n    console.log(acc, curr, index, src);\n    return (acc += curr);\n  },\n  0\n);\n\nconsole.log(result);"]],"language":[["JavaScript"]]},"format":{"code_wrap":true},"created_time":1673275165532,"last_edited_time":1673276541375,"